<%- include("partial/header"); -%>

<div class="">
  <h1> Home </h1>
  <p><%= startingContent %></p>
</div>

<div class="target">
  <% posts.forEach(function(post){ %>
    <h1><%= post.title %> </h1>
    <p>
      <%= post.content.substring(0,100) + "..." %>
      <a href="/posts/<%=post.title %>">Read more</a>
    </p>

  <% }) %>
</div>


<%- include("partial/footer"); -%>
